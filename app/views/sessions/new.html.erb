<h1>Login below</h1>
<div class="login_form">
    <%= form_with url: "/login", method: :post do |form| %>
        <%= form.label :email %>
        <%= form.email_field :email %><br>
        <%= form.label :password %>
        <%= form.password_field :password %>
        <%= form.submit "Login" %>
    <% end %>
</div><br><br>
<h1>Sign in with Google</h1>
<div id="google_login_buton">
    <%=button_to "Google Login", '/auth/google_oauth2'%>
</div>
